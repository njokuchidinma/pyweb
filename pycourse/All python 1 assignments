    oh = 40

hrs = input("Enter Hours:")
rte = input("Enter Rate:")
h = float(hrs)
r = float(rte)

if h > oh:

   pay = oh * r
   ext = (h-oh) * (r * 1.5)

   fp = pay + ext

else:
    fp = oh * r
   
print(fp)
