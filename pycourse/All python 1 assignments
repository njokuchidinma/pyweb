ex 05_0

num = 0
tot = 0.0
while True :
    sval = input('Enter a number: ')
    if sval == 'done' :
        break
    try:
        fval = float(sval)
    except:
        print('Invalid input')
        continue
    # print('fval')
    num = num + 1
    tot = tot + fval
    
 # print ('ALL DONE')   
print(tot,num,tot/num)
 assignment 5.2
 
 numbers_list = []

while True:
    try:
          num = input("Enter a number: ")
          nums = int(num)
          numbers_list.append(nums)
    except:
          if num == "done":
              break
          else:
              print("Invalid input")  
largest = (max(numbers_list))
smallest = (min(numbers_list))
print("Maximum is", largest)
print("Minimum is", smallest)
 
